{
  "version": "1.7.0",
  "disk_read": false,
  "disk_write": false,
  "unc_read": false,
  "unc_write": false,
  "request_post_limit": "100mb",
  "gzip_level": "9",
  "brotli_quality": "11",
  "jpeg_quality": "100",
  "tinypng_api_key": "GMjX2yh2dnWMkWmdpFtl4tktgv8TMR5s",
  "env": "development",
  "port": {
    "production": "80",
    "development": "3000"
  },
  "external": {
    "html": {
      "html_minifier": {
        "minify": {
          "collapseWhitespace": true,
          "collapseBooleanAttributes": true,
          "removeEmptyAttributes": true,
          "removeRedundantAttributes": true,
          "removeScriptTypeAttributes": true,
          "removeStyleLinkTypeAttributes": true,
          "removeComments": true
        }
      },
      "js_beautify": {
        "beautify": {
          "wrap_line_length": 0
        }
      }
    },
    "css": {
      "clean_css": {
        "beautify": { "level": 0, "format": "beautify" },
        "minify": {}
      },
      "js_beautify": {}
    },
    "js": {
      "js_beautify": {
        "beautify": {
          "break_chained_methods": true,
          "keep_array_indentation": true
        }
      },
      "terser": {
        "minify": {
          "keep_classnames": true
        }
      }
    }
  },
  "routing": {
    "shared": [
      { "mount": "html", "path": "/" },
      { "mount": "dist", "path": "/dist" }
    ],
    "production": [],
    "development": [
      { "mount": "build", "path": "/build" },
      { "mount": "html/common", "path": "/common" },
      { "mount": "html/books", "path": "/books" },
      { "mount": "html/demos", "path": "/demos" },
      { "mount": "html/demos-dev", "path": "/demos-dev" }
    ]
  }
}