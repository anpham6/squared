{
  "version": "2.1.3",
  "disk_read": false,
  "disk_write": false,
  "unc_read": false,
  "unc_write": false,
  "env": "development",
  "port": {
    "production": "80",
    "development": "3000"
  },
  "routing": {
    "__SHARED__": [
      { "mount": "html", "path": "/" },
      { "mount": "dist", "path": "/dist" }
    ],
    "production": [],
    "development": [
      { "mount": "build", "path": "/build" },
      { "mount": "temp", "path": "/temp" }
    ]
  },
  "cors": {
    "origin": false
  },
  "request_post_limit": "250mb",
  "watch_interval": 200,
  "compress": {
    "gzip_level": 9,
    "brotli_quality": 11,
    "tinypng_api_key": ""
  },
  "cloud": {
    "s3": {
      "main": {
        "accessKeyId": "",
        "secretAccessKey": ""
      }
    },
    "azure": {
      "main": {
        "accountName": "",
        "accountKey": ""
      }
    },
    "gcs": {
      "main": {
        "keyFilename": ""
      }
    },
    "oci": {
      "main": {
        "region": "",
        "namespace": "",
        "accessKeyId": "",
        "secretAccessKey": ""
      }
    },
    "ibm": {
      "main": {
        "apiKeyId": "",
        "serviceInstanceId": "",
        "endpoint": "",
        "region": ""
      }
    }
  },
  "gulp": {
    "minify": "./gulpfile.js"
  },
  "chrome": {
    "eval_function": true,
    "eval_text_template": true,
    "html": {
      "posthtml": {
        "transform": {
          "plugins": []
        },
        "transform-output": {}
      },
      "prettier": {
        "beautify": {
          "parser": "html",
          "printWidth": 120,
          "tabWidth": 4
        }
      },
      "html-minifier-terser": {
        "minify": {
          "collapseWhitespace": true,
          "collapseBooleanAttributes": true,
          "removeEmptyAttributes": true,
          "removeRedundantAttributes": true,
          "removeScriptTypeAttributes": true,
          "removeStyleLinkTypeAttributes": true,
          "removeComments": true
        }
      }
    },
    "css": {
      "postcss": {
        "transform": {
          "plugins": []
        }
      },
      "prettier": {
        "beautify": {
          "parser": "css",
          "tabWidth": 4
        }
      },
      "clean-css": {
        "minify": {
          "inline": ["none"],
          "level": 1
        }
      }
    },
    "js": {
      "@babel/core": {
        "es5": {
          "presets": ["@babel/preset-env"]
        }
      },
      "prettier": {
        "beautify": {
          "parser": "babel",
          "printWidth": 120,
          "tabWidth": 4
        }
      },
      "terser": {
        "minify": {
          "toplevel": true,
          "sourceMap": false
        }
      },
      "uglify-js": {
        "es5-minify": {
          "toplevel": true
        }
      },
      "rollup": {
        "bundle": {
          "treeshake": false,
          "external": [],
          "output": {
            "sourcemap": false
          }
        }
      }
    }
  }
}