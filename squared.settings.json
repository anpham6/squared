{
  "version": "1.7.0",
  "disk_read": false,
  "disk_write": false,
  "unc_read": false,
  "unc_write": false,
  "request_post_limit": "100mb",
  "gzip_level": "9",
  "brotli_quality": "11",
  "jpeg_quality": "100",
  "tinypng_api_key": "",
  "env": "development",
  "port": {
    "production": "80",
    "development": "3000"
  },
  "routing": {
    "shared": [
      { "mount": "html", "path": "/" },
      { "mount": "dist", "path": "/dist" }
    ],
    "production": [],
    "development": [
      { "mount": "build", "path": "/build" },
      { "mount": "html/common", "path": "/common" },
      { "mount": "html/books", "path": "/books" },
      { "mount": "html/demos", "path": "/demos" },
      { "mount": "html/demos-dev", "path": "/demos-dev" }
    ]
  }
}