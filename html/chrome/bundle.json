[
  {
    "selector": "html",
    "type": "html",
    "filename": "index.html",
    "process": [
      "beautify"
    ],
    "tasks": [
      "minify"
    ],
    "attributes": [
      {
        "name": "lang",
        "value": "en"
      }
    ]
  },
  {
    "selector": "head > link:nth-of-type(1)",
    "type": "css",
    "inline": true,
    "process": [
      "minify"
    ]
  },
  {
    "selector": "head > style:nth-of-type(1)",
    "type": "css",
    "exportAs": "css/prod.css",
    "process": [
      "minify",
      "preserve"
    ]
  },
  {
    "selector": "head > style:nth-of-type(2)",
    "type": "css",
    "exportAs": "css/prod.css",
    "process": [
      "minify"
    ],
    "preserve": true
  },
  {
    "selector": "head > script:nth-of-type(1)",
    "type": "js",
    "saveAs": "js/modules1.js",
    "process": [
      "bundle",
      "beautify"
    ]
  },
  {
    "selector": "head > script:nth-of-type(2), head > script:nth-of-type(3)",
    "type": "js",
    "saveAs": "js/modules2.js"
  },
  {
    "selector": "head > script:nth-of-type(4)",
    "type": "js",
    "exportAs": "js/modules3.js",
    "process": [
      "es5-script",
      "beautify"
    ]
  },
  {
    "selector": "head > script:nth-of-type(5)",
    "type": "js",
    "exclude": true,
    "template": {
      "module": "@babel/core",
      "identifier": "es5-script"
    }
  },
  {
    "selector": ".card:nth-of-type(1) img",
    "type": "image",
    "pathname": "../images/harbour",
    "commands": [
      "png(10000,75000)(100x200^cover){90,180,270#FFFFFF}",
      "webp(300x100^contain){90,180,270#FFFFFF}|100[picture]|"
    ],
    "attributes": [
      {
        "name": "alt",
        "value": "Picture 1"
      }
    ]
  },
  {
    "selector": ".card:nth-of-type(2) img",
    "type": "image",
    "commands": [
      "jpeg@(500x500[bezier]^contain[right|bottom])(-50,100|200x200){225#FFFFFF}|50|"
    ],
    "attributes": [
      {
        "name": "alt",
        "value": "Picture 2"
      }
    ]
  },
  {
    "selector": ".card:nth-of-type(3) img",
    "type": "image",
    "pathname": "../images/harbour",
    "commands": [
      "png@(25000,*)(800x600[bezier]^contain[right|top]){315}|0.5|"
    ],
    "attributes": [
      {
        "name": "alt",
        "value": "Picture 3"
      }
    ]
  },
  {
    "selector": "object",
    "type": "image",
    "base64": true
  },
  {
    "selector": "embed",
    "type": "image",
    "pathname": "../images/harbour",
    "commands": [
      "png@(100x200^contain[right|middle]){45#FFFFFF}|0.5|"
    ]
  }
]